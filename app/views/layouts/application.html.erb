	<!--being Optimize CAPI tag -->
	<script src="assets/wt/wt_capi.js" type="text/javascript"></script>
	<!-- end Optimize CAPI tag-->

	<!--being Analytics/VDM tag -->
	<script src="assets/wt/webtrends.js" type="text/javascript"></script>
	<!-- end Analytics/VDM tag -->

